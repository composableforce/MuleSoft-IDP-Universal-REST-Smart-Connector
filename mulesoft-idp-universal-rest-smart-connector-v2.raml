types: # Considered use of Library, but the dependency was deemed to hinder adoption

  field-apiVersion:
    displayName: API Version
    description:
    type: string
    enum:
      - v1
    example: v1
    default: v1

  field-orgId:
    displayName: MuleSoft Anypoint Organization Id
    description: |
      Your Anypoint Organisation Id see: [Find your Organization ID.](https://docs.mulesoft.com/access-management/organization#find-your-organization-id)
    example: eca25329-9592-4ff1-9054-1b08d103b991
    pattern: ^[{]?[0-9a-fA-F]{8}-([0-9a-fA-F]{4}-){3}[0-9a-fA-F]{12}[}]?$
    
  field-actionId:
    displayName: IDP Document Action Id
    description: Published Document Action UUID
    example: 90c3e286-72b5-42ca-92d0-22f86ea8d766
    type: string
    pattern: ^[{]?[0-9a-fA-F]{8}-([0-9a-fA-F]{4}-){3}[0-9a-fA-F]{12}[}]?$

  field-actionVersion:
    displayName: IDP Document Action Id Version
    description: Document Action Version like 1.0.0
    example: 1.1.0
    type: string

  field-executionId:
    displayName: IDP Execution Id
    description: IDP Execution Id returned after successful submission
    example: 022c61de-9bfc-4a94-8a9c-43c5f8ac153e
    type: string
    pattern: ^[{]?[0-9a-fA-F]{8}-([0-9a-fA-F]{4}-){3}[0-9a-fA-F]{12}[}]?$

  field-ExecutionStatus:
    displayName: IDP Execution Status
    description: |
      ## [Execution Results Reference](https://docs.mulesoft.com/idp/automate-document-processing-with-the-idp-api#execution-status-reference).

      When you query the results of an execution, the IDP API returns the status which can be any of the following:

      - `ACKNOWLEDGED`: The document action execution request was received.
      - `IN_PROGRESS`: The execution started.
      - `RESULTS_PENDING`: The execution finished and IDP is processing the results.
      - `MANUAL_VALIDATION_REQUIRED`: The execution finished but the results need manual validation.
      - `FAILED`: The execution request finished unsuccessfully.
      - `PARTIAL_SUCCESS`: The execution request finished but some sub-tasks failed.
      - `SUCCEEDED`: The execution request finished successfully.
    default: IN_PROGRESS
    enum:
      - ACKNOWLEDGED
      - IN_PROGRESS
      - RESULTS_PENDING
      - MANUAL_VALIDATION_REQUIRED
      - FAILED
      - PARTIAL_SUCCESS
      - SUCCEEDED
    example: IN_PROGRESS
    type: string

  field-uuid:
    type: string
    pattern: ^[{]?[0-9a-fA-F]{8}-([0-9a-fA-F]{4}-){3}[0-9a-fA-F]{12}[}]?$

  object-action:
    type: object
    properties:
      id:
        type: field-actionId
        required: true
      organizationId:
        type: field-orgId
        required: true
      name:
        type: string
        example: IDP-PO-CD656092-BurlingtonTextiles
        required: true
      description:
        type: string
        example: CD656092-BurlingtonTextiles Purchase Order
        required: false
      "type":
        type: string
        enum:
          - GENERIC
          - INVOICE
          - PURCHASE_ORDER
        example: GENERIC
        required: true
      empowerWith:
        type: string
        required: false
        enum:
          - NLP
      configuration:
        type: object-actionConfiguration
      createdBy:
        type: field-uuid
        example: 4d4fda9b-b772-4085-ac61-078af3b54543
        required: true
      createdAt:
        type: datetime
        example: '2024-06-24T16:56:37.372612Z'
        format: rfc3339
        required: true
      updatedAt:
        type: datetime
        example: '2024-06-24T17:03:55.950227Z'
        format: rfc3339
        required: false
      registration:
        type: object
        properties:
          status:
            type: string
            enum:
              - REGISTERED
            example: REGISTERED
            required: false
          message:
            type: string
            example: ''
            required: false

  object-actionConfiguration:
    type: object
    properties:
      content:
        type: object
        properties:
          output:
            type: object
            properties:
              fields:
                type: object
              tables:
                type: object
              prompts::
                type: Prompts
      version:
        type: field-actionVersion

  Prompts:
    type: object
    properties:
      /^[a-zA-Z0-9_.-]*$/:
        type: Prompt
  Prompt:
    type: object
    properties:
      hidden:
        type: boolean
        example: false
      prompt:
        type: string
        example: what is the text
      source:
        type: string
        enum:
         - llm
         - document
        example: document
      required:
        type: boolean
        example: true
      confidenceThreshold:
        type: integer
        example: 10

  object-actionOperation:
    type: object
    properties:
      id:
        type: field-uuid
      "type":
        type: string
        enum:
          - EXCHANGE_PUBLICATION
          - RUNTIME_PUBLICATION
        example: EXCHANGE_PUBLICATION
      status:
        type: string
        example: SUCCESS
      result:
        type: string
      info:
        type: object
        properties:
          baseUrl:
            type: string
            example: "https://idp-rt.us-east-1.anypoint.mulesoft.com"
            required: false
      createdAt:
        type: datetime
        example: '2024-06-24T16:56:37.372612Z'
        format: rfc3339
        required: true
      updatedAt:
        type: datetime
        example: '2024-06-24T17:03:55.950227Z'
        format: rfc3339
        required: false

  object-actionsList:
    type: object
    properties:
      actions:
        type: object-action []
      total:
        description: Total Org's Actions (not zero based)
        type: integer
        example: 6
        required: true

  object-actionVersion:
    type: object
    properties:
      organizationId:
        type: field-orgId
        required: true
      actionId:
        type: field-actionId
        required: true
      version:
        type: field-actionVersion
        required: true
      status:
        type: string
        example: SUCCESS
        required: false
      configuration:
        type: object-actionConfiguration
      operations:
        type: object-actionOperation []
      createdAt:
        type: datetime
        example: '2024-06-24T16:56:37.372612Z'
        format: rfc3339
        required: true
      updatedAt:
        type: datetime
        example: '2024-06-24T17:03:55.950227Z'
        format: rfc3339
        required: false

  object-DocumentExecutionPostResponse:
    displayName: IDP Document Submission Response Object
    description: 
    type: object
    properties:
      id:
        type: field-executionId
        required: true
      documentName:
        description: Document Name and Extension
        type: string
        example: IDP-PO-CD656092-BurlingtonTextiles.pdf
        required: true
      status:
        type: field-ExecutionStatus
        required: false
    additionalProperties: false
    example: {"id": "af78b8b2-3867-4073-aac6-d24e01f48ae2","documentName": "IDP-PO-CD656092-BurlingtonTextiles.pdf"}

  object-DocumentExecutionGetResponse:
    displayName: IDP Retrieve Execution Response Object
    type: object
    description:
    properties:
      pages:
        required: false
        type: array
        items: object-Document-Page
      id:
        type: field-executionId
        required: false
      documentName:
        type: string
        required: false
      prompts:
        type: object-Document-Prompt
        required: false
      status:
        type: field-ExecutionStatus
        required: false

  object-Document-Page:
    displayName: IDP Retrieve Execution Response Page Object
    type: object
    properties:
      tables?:
        type: object
        properties: {}
      page?:
        type: string
      fields?:
        type: object
        properties: {}
      prompts?:
        type: object-Document-Prompt

  object-Document-Prompt:
    displayName: IDP Retrieve Execution Response Prompt Object
    type: object
    properties:
      answer?: object-Document-Field
      source?:
        type: string
      prompt?:
        type: string

  object-Document-Field:
    displayName: IDP Retrieve Execution Response Field Object
    type: object
    properties:
      value?:
        type: string
      geometry?:
        type: object
        properties:
          height:
            type: number
            required: false
          left:
            type: number
            required: false
          width:
            type: number
            required: false
          top:
            type: number
            required: false
      confidenceScore?:
        type: number

  object-reviewTask:
    displayName: IDP Review Task
    description: IDP Review Task
    properties:
        results: 
          type: array
        queriesResults: 
          items:
            type:
              properties:
                id: string
                name: string
                query: string
                result: nil
                confidenceScore: nil
                page: number
                geometry: nil
        analyzersResults: 
          type: array
        signaturesResults: 
          type: array

  object-commonError:
    displayName: üÜò IDP Common Error Response
    description: IDP Common Error Response
    properties:
      status:
        description: Number indicating the HTTP status code generated by the origin server for this occurrence of the problem
        default: 404
        example: 404
        type: number
        format: int32
        required: false
      title:
        description: Short, human-readable summary of the problem type
        example: Not Found
        type: string
        required: false
      detail:
        description: Human-readable explanation specific to this occurrence of the problem
        example: "Action Id: 90c3e286-72b5-42ca-92d0-22f86ea8d766, Version: 1.1.0, Execution Id: 022c61de-9bfc-4a94-8a9c-43c5f8ac1531"
        type: string
        required: false
      type:
        description: URI reference that identifies the problem type
        required: false
        type: string
    example: {"status": 404,"title": "Not Found"}
    additionalProperties: false







  object-DocumentExecutionPostRequestBase64:
    description: Request object for executing an action with a Base64-encoded file
    type: object
    properties:
      callback:
        description: Callback Url
        required: false
        example:
          strict: false
          value: "{\"noAuthUrl\":\"callbackURL\"}"
        type: string
      file:
        description: File content in base64 string format
        required: true
        example:
          strict: false
          value: 
        type: string
      fileName:
        required: false
        description: File name provided by user
        example:
          strict: false
          value: testFile.pdf
        type: string
  object-FilteredLlmAnalyzerLowConfidenceResult:
    description: Analyzers that need review
    type: object
    properties:
      confidenceScore:
        type: number
        format: float
        required: true
      id:
        type: string
        required: true
      isValidated?:
        type: boolean
      name:
        type: string
        required: true
      query:
        type: string
        required: true
      result:
        type: string
        required: true
  object-ReviewItem:
    description: Review Item Response
    type: object
    properties:
      docActionId:
        description: Document Action Id
        required: false
        type: string
      docFileId:
        description: Document File Id
        required: false
        type: string
      docFileName:
        description: Document FileName
        required: false
        type: string
      docOperationCreationDate:
        description: Document Operation Creation Date
        required: false
        type: datetime
      docOperationId:
        description: Document Operation Id
        required: false
        type: string
  object-DocumentActionLowConfidenceExecution:
    description: Low Confidence Items/Queries/Analyzers/Signatures
    type: object
    example:
      strict: false
      value:
        results: []
        queriesResults: []
        analyzersResults: []
        signaturesResults: []
    properties:
      analyzersResults:
        description: Analyzers that need review
        default: []
        type: array
        items: object-FilteredLlmAnalyzerLowConfidenceResult
        required: true
      queriesResults: object-Json
      results: object-Json
      signaturesResults: object-Json

  object-JsonNode:
    type: object
  object-Json:
    description: Signatures with Low Confidence Scores
    type: object
  object-Pageable:
    type: object
    properties:
      page:
        type: number
        minimum: 0
        format: int32
        required: false
      size:
        type: number
        minimum: 1
        format: int32
        required: false
      sort:
        type: string
        enum:
          - updatedAt,desc
